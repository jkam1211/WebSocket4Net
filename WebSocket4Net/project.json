{
  "version": "0.14.1.0",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": false,
	"compile": {
      "exclude": [
          "Properties/*.cs",
          "**/*.Net20.cs",
          "**/*.Silverlight.cs",
          "**/*.Net40.cs",
          "**/*.Net35.cs",
          "**/*.WindowsPhone.cs"
        ],
      "excludeFiles": [
          "GlobalAssemblyInfo.cs",
		  "JsonWebSocket.DataContractJson.cs",
		  "JsonWebSocket.JsonNet.cs",
		  "Func.cs",
		  "Action.cs",
		  "LINQ.cs"
        ]
    }
  },
  "dependencies": {
      "SuperSocket.ClientEngine": "[0.8.0.2]"
  },
  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "System.Linq": "4.1.0",
        "System.Net.Sockets": "4.1.0",
        "System.Net.NameResolution": "4.0.0",
		"System.Threading.Timer": "4.0.1",
		"System.Text.RegularExpressions": "4.1.0",
		"System.Threading": "4.0.11"
      }
    }
  }
}
